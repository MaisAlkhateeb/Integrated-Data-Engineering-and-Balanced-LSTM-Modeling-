   "source": [
    "# ==============================================================\n",
    "# QCRI-Style Daily & Patient-Level Aggregation + Multi-Feature Boxplots\n",
    "# ==============================================================\n",
    "\n",
    "import pandas as pd\n",
    "import numpy as np\n",
    "import matplotlib.pyplot as plt\n",
    "from pathlib import Path\n",
    "\n",
    "# ==============================================================\n",
    "# 1. CONFIGURATION\n",
    "# ==============================================================\n",
    "CSV_PATH = \"/kaggle/input/hmcdataset/intraday.csv\"\n",
    "OUT_DAILY_CSV = \"/kaggle/working/daily_summary_qcri.csv\"\n",
    "OUT_PATIENT_CSV = \"/kaggle/working/qcri_patient_summary.csv\"\n",
    "OUT_PLOTS_DIR = Path(\"/kaggle/working/hypo_boxplots\")\n",
    "\n",
    "# All potential intraday features in your dataset\n",
    "ALL_FEATURES = [\n",
    "    \"calories\", \"cgm\", \"deep\", \"distance\", \"heart_rate\",\n",
    "    \"light\", \"nap\", \"rem\", \"spo2\", \"steps\", \"awake\"\n",
    "]\n",
    "\n",
    "# ==============================================================\n",
    "# 2. LOAD & PREPROCESS DATA\n",
    "# ==============================================================\n",
    "df = pd.read_csv(CSV_PATH)\n",
    "df[\"start\"] = pd.to_datetime(df.get(\"start\"), errors=\"coerce\")\n",
    "df[\"date\"] = df[\"start\"].dt.date\n",
    "df[\"hour\"] = df[\"start\"].dt.floor(\"h\")\n",
    "df[\"hour_of_day\"] = df[\"start\"].dt.hour\n",
    "\n",
    "# Ensure all numerical columns are numeric\n",
    "for col in ALL_FEATURES:\n",
    "    if col in df.columns:\n",
    "        df[col] = pd.to_numeric(df[col], errors=\"coerce\")\n",
    "\n",
    "# Keep only rows with CGM readings\n",
    "if \"cgm\" not in df.columns:\n",
    "    raise ValueError(\"Missing required column: `cgm`\")\n",
    "\n",
    "df_cgm = df.dropna(subset=[\"cgm\"]).copy()\n",
    "\n",
    "# Add hypoglycemia flag (‚â§70 mg/dL)\n",
    "df_cgm[\"hypo\"] = df_cgm[\"cgm\"] <= 70\n",
    "\n",
    "# ==============================================================\n",
    "# 3. DAILY AGGREGATION (QCRI-style)\n",
    "# ==============================================================\n",
    "agg_map = {\n",
    "    \"cgm_mean\": (\"cgm\", \"mean\"),\n",
    "    \"cgm_sd\": (\"cgm\", \"std\"),\n",
    "    \"hypo_percent\": (\"hypo\", \"mean\"),  # proportion of readings ‚â§70\n",
    "}\n",
    "\n",
    "# Add sum-type daily totals if columns exist\n",
    "for feature, out_col in [\n",
    "    (\"steps\", \"total_steps\"),\n",
    "    (\"distance\", \"total_distance\"),\n",
    "    (\"calories\", \"total_calories\"),\n",
    "]:\n",
    "    if feature in df_cgm.columns:\n",
    "        agg_map[out_col] = (feature, \"sum\")\n",
    "\n",
    "# Add sleep metrics (sum of minutes per stage)\n",
    "for stage in [\"awake\", \"deep\", \"light\", \"nap\", \"rem\"]:\n",
    "    if stage in df_cgm.columns:\n",
    "        agg_map[f\"sleep_{stage}_min\"] = (stage, \"sum\")\n",
    "\n",
    "# Add physiology metrics (mean per day)\n",
    "if \"heart_rate\" in df_cgm.columns:\n",
    "    agg_map[\"avg_heart_rate\"] = (\"heart_rate\", \"mean\")\n",
    "if \"spo2\" in df_cgm.columns:\n",
    "    agg_map[\"avg_spo2\"] = (\"spo2\", \"mean\")\n",
    "\n",
    "# Perform aggregation\n",
    "daily_summary = df_cgm.groupby([\"patientID\", \"date\"], as_index=False).agg(**agg_map)\n",
    "\n",
    "# Label hypoglycemic days (‚â•4% of readings ‚â§70)\n",
    "daily_summary[\"hypo_day\"] = daily_summary[\"hypo_percent\"] >= 0.04\n",
    "\n",
    "# Combine total sleep minutes per day\n",
    "sleep_cols = [c for c in daily_summary.columns if c.startswith(\"sleep_\") and c.endswith(\"_min\")]\n",
    "if sleep_cols:\n",
    "    daily_summary[\"sleep_total_min\"] = daily_summary[sleep_cols].sum(axis=1)\n",
    "\n",
    "# ==============================================================\n",
    "# 4. PATIENT-LEVEL AGGREGATION\n",
    "# ==============================================================\n",
    "patient_aggs = {\n",
    "    \"total_days\": (\"date\", \"nunique\"),\n",
    "    \"hypo_days\": (\"hypo_day\", \"sum\"),\n",
    "}\n",
    "\n",
    "# Add average of all numeric daily metrics\n",
    "daily_metrics = [\n",
    "    \"total_steps\", \"total_distance\", \"total_calories\",\n",
    "    \"avg_heart_rate\", \"avg_spo2\", \"sleep_total_min\",\n",
    "    \"cgm_mean\", \"cgm_sd\"\n",
    "] + sleep_cols\n",
    "daily_metrics = [c for c in daily_metrics if c in daily_summary.columns]\n",
    "\n",
    "for col in daily_metrics:\n",
    "    patient_aggs[f\"avg_{col}\"] = (col, \"mean\")\n",
    "\n",
    "qcri_summary = daily_summary.groupby(\"patientID\", as_index=False).agg(**patient_aggs)\n",
    "qcri_summary[\"hypo_day_percent\"] = (qcri_summary[\"hypo_days\"] / qcri_summary[\"total_days\"]) * 100\n",
    "\n",
    "# Round for presentation\n",
    "for col in qcri_summary.columns:\n",
    "    if col.startswith(\"avg_\") or col == \"hypo_day_percent\":\n",
    "        qcri_summary[col] = qcri_summary[col].round(3)\n",
    "\n",
    "# ==============================================================\n",
    "# 5. VALIDATION SUMMARY TABLE\n",
    "# ==============================================================\n",
    "rows = [\n",
    "    (\"Total Days (avg)\", round(qcri_summary[\"total_days\"].mean(), 3)),\n",
    "    (\"% Hypoglycemic Days (range)\",\n",
    "     f\"{qcri_summary['hypo_day_percent'].min():.3f}% to {qcri_summary['hypo_day_percent'].max():.3f}%\"),\n",
    "]\n",
    "\n",
    "# Add feature-specific ranges\n",
    "for label, col in [\n",
    "    (\"Avg. Daily Steps (range)\", \"avg_total_steps\"),\n",
    "    (\"Avg. Daily Distance (range)\", \"avg_total_distance\"),\n",
    "    (\"Avg. Daily Calories (range)\", \"avg_total_calories\"),\n",
    "    (\"Avg. Heart Rate (range)\", \"avg_avg_heart_rate\"),\n",
    "    (\"Avg. SpO2 (range)\", \"avg_avg_spo2\"),\n",
    "    (\"Avg. Total Sleep (min) (range)\", \"avg_sleep_total_min\"),\n",
    "]:\n",
    "    if col in qcri_summary.columns:\n",
    "        rows.append((label, f\"{qcri_summary[col].min():.3f} to {qcri_summary[col].max():.3f}\"))\n",
    "\n",
    "validation_table = pd.DataFrame(rows, columns=[\"Metric\", \"Computed Range\"])\n",
    "print(\"\\nQCRI-Style Validation Table:\\n\", validation_table)\n",
    "\n",
    "# ==============================================================\n",
    "# 6. VISUALIZATIONS\n",
    "# ==============================================================\n",
    "OUT_PLOTS_DIR.mkdir(parents=True, exist_ok=True)\n",
    "features_to_plot = [\n",
    "    (\"Total Steps\", \"total_steps\"),\n",
    "    (\"Total Distance\", \"total_distance\"),\n",
    "    (\"Total Calories\", \"total_calories\"),\n",
    "    (\"Avg Heart Rate\", \"avg_heart_rate\"),\n",
    "    (\"Avg SpO2\", \"avg_spo2\"),\n",
    "    (\"Total Sleep (min)\", \"sleep_total_min\"),\n",
    "]\n",
    "\n",
    "saved_plots = []\n",
    "\n",
    "# --- Boxplots for all features vs. hypo_day ---\n",
    "for label, col in features_to_plot:\n",
    "    if col not in daily_summary.columns:\n",
    "        continue\n",
    "\n",
    "    plt.figure(figsize=(6, 6))\n",
    "    plt.boxplot(\n",
    "        [\n",
    "            daily_summary.loc[~daily_summary[\"hypo_day\"], col].dropna(),\n",
    "            daily_summary.loc[daily_summary[\"hypo_day\"], col].dropna(),\n",
    "        ],\n",
    "        labels=[\"Non-Hypo Day\", \"Hypo Day\"],\n",
    "        showmeans=True,\n",
    "    )\n",
    "    plt.title(f\"{label} on Hypo vs Non-Hypo Days\")\n",
    "    plt.ylabel(label)\n",
    "    plt.tight_layout()\n",
    "\n",
    "    out_path = OUT_PLOTS_DIR / f\"box_{col}.png\"\n",
    "    plt.savefig(out_path, dpi=150)\n",
    "    plt.close()\n",
    "    saved_plots.append(out_path)\n",
    "\n",
    "print(f\"\\n‚úÖ Saved {len(saved_plots)} boxplots to {OUT_PLOTS_DIR}\")\n",
    "\n",
    "# ==============================================================\n",
    "# 7. HOURLY HYPOGLYCEMIA ANALYSIS\n",
    "# ==============================================================\n",
    "hourly_hypo = (\n",
    "    df_cgm.groupby(\"hour_of_day\", as_index=False)[\"hypo\"]\n",
    "    .mean()\n",
    "    .sort_values(\"hour_of_day\")\n",
    ")\n",
    "\n",
    "plt.figure(figsize=(12, 6))\n",
    "plt.plot(hourly_hypo[\"hour_of_day\"], hourly_hypo[\"hypo\"], marker=\"o\")\n",
    "plt.title(\"Hourly Hypoglycemia Frequency Across All Patients\")\n",
    "plt.xlabel(\"Hour of Day (0 = Midnight)\")\n",
    "plt.ylabel(\"Hypoglycemia Rate\")\n",
    "plt.xticks(range(0, 24))\n",
    "plt.grid(True)\n",
    "plt.tight_layout()\n",
    "plt.show()\n",
    "\n",
    "# ==============================================================\n",
    "# 8. SAVE OUTPUTS\n",
    "# ==============================================================\n",
    "daily_summary.to_csv(OUT_DAILY_CSV, index=False)\n",
    "qcri_summary.to_csv(OUT_PATIENT_CSV, index=False)\n",
    "\n",
    "print(f\"\\nSaved daily summary ‚Üí {OUT_DAILY_CSV}\")\n",
    "print(f\"Saved per-patient QCRI summary ‚Üí {OUT_PATIENT_CSV}\")\n"
   ]
