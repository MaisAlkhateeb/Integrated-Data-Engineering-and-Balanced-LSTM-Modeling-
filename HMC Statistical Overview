    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "\n",
      "Saved daily summary to: /kaggle/working/daily_summary_qcri.csv\n",
      "Saved per-patient QCRI summary to: /kaggle/working/qcri_patient_summary.csv\n"
