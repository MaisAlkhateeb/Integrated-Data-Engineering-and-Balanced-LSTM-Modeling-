   ],
   "source": [
    "import pandas as pd\n",
    "import numpy as np\n",
    "import matplotlib.pyplot as plt\n",
    "from pathlib import Path\n",
    "\n",
    "# =========================\n",
    "# CONFIG\n",
    "# =========================\n",
    "CSV_PATH = \"/kaggle/input/hmcdataset/intraday.csv\"\n",
    "OUT_DAILY_CSV = \"/kaggle/working/daily_summary_qcri.csv\"\n",
    "OUT_PATIENT_CSV = \"/kaggle/working/qcri_patient_summary.csv\"\n",
    "\n",
    "# All possible intraday features in your file\n",
    "ALL_FEATURES = [\n",
    "    \"calories\", \"cgm\", \"deep\", \"distance\", \"heart_rate\",\n",
    "    \"light\", \"nap\", \"rem\", \"spo2\", \"steps\", \"awake\"\n",
    "]\n",
    "\n",
    "# =========================\n",
    "# LOAD & PREP (CGM-only universe)\n",
    "# =========================\n",
    "df = pd.read_csv(CSV_PATH)\n",
    "\n",
    "# Parse timestamps\n",
    "df[\"start\"] = pd.to_datetime(df.get(\"start\"), errors=\"coerce\")\n",
    "df[\"date\"]  = df[\"start\"].dt.date\n",
    "df[\"hour\"]  = df[\"start\"].dt.floor(\"h\")   # for hourly analysis\n",
    "df[\"hour_of_day\"] = df[\"start\"].dt.hour\n",
    "\n",
    "# Coerce features to numeric (keeps NaN if non-numeric)\n",
    "for col in ALL_FEATURES:\n",
    "    if col in df.columns:\n",
    "        df[col] = pd.to_numeric(df[col], errors=\"coerce\")\n",
    "\n",
    "# ---- CGM-only ----\n",
    "if \"cgm\" not in df.columns:\n",
    "    raise ValueError(\"`cgm` column is required for hypoglycemia detection but is missing.\")\n",
    "df_cgm = df.dropna(subset=[\"cgm\"]).copy()\n",
    "\n",
    "# Reading-level hypoglycemia flag (‚â§70 mg/dL)\n",
    "df_cgm[\"hypo\"] = df_cgm[\"cgm\"] <= 70\n",
    "\n",
    "# =========================\n",
    "# DAILY AGGREGATION (QCRI-style)\n",
    "# =========================\n",
    "# Build a dynamic aggregation dict based on present columns\n",
    "agg_map = {\n",
    "    # CGM metrics\n",
    "    \"cgm_mean\": (\"cgm\", \"mean\"),\n",
    "    \"cgm_sd\":   (\"cgm\", \"std\"),\n",
    "    # Hypo %\n",
    "    \"hypo_percent\": (\"hypo\", \"mean\"),\n",
    "}\n",
    "\n",
    "# Sum-type features (daily totals)\n",
    "if \"steps\" in df_cgm.columns:     agg_map[\"total_steps\"] = (\"steps\", \"sum\")\n",
    "if \"distance\" in df_cgm.columns:  agg_map[\"total_distance\"] = (\"distance\", \"sum\")\n",
    "if \"calories\" in df_cgm.columns:  agg_map[\"total_calories\"] = (\"calories\", \"sum\")\n",
    "\n",
    "# Sleep minutes (sum per day)\n",
    "for sl in [\"awake\", \"deep\", \"light\", \"nap\", \"rem\"]:\n",
    "    if sl in df_cgm.columns:\n",
    "        agg_map[f\"sleep_{sl}_min\"] = (sl, \"sum\")\n",
    "\n",
    "# Mean-type features (physiology)\n",
    "if \"heart_rate\" in df_cgm.columns: agg_map[\"avg_heart_rate\"] = (\"heart_rate\", \"mean\")\n",
    "if \"spo2\" in df_cgm.columns:       agg_map[\"avg_spo2\"] = (\"spo2\", \"mean\")\n",
    "\n",
    "daily_summary = (\n",
    "    df_cgm\n",
    "    .groupby([\"patientID\", \"date\"], as_index=False)\n",
    "    .agg(**agg_map)\n",
    ")\n",
    "\n",
    "# Hypo day label per QCRI rule: ‚â•4% of readings ‚â§70 on that day\n",
    "daily_summary[\"hypo_day\"] = daily_summary[\"hypo_percent\"] >= 0.04\n",
    "\n",
    "# Optional: total sleep minutes per day (if stages present)\n",
    "sleep_cols = [c for c in daily_summary.columns if c.startswith(\"sleep_\") and c.endswith(\"_min\")]\n",
    "if sleep_cols:\n",
    "    daily_summary[\"sleep_total_min\"] = daily_summary[sleep_cols].sum(axis=1)\n",
    "\n",
    "# =========================\n",
    "# PER-PATIENT (QCRI harmonized)\n",
    "# =========================\n",
    "patient_aggs = {\n",
    "    \"total_days\": (\"date\", \"nunique\"),\n",
    "    \"hypo_days\":  (\"hypo_day\", \"sum\"),\n",
    "}\n",
    "\n",
    "# Add averages of daily metrics per patient (only for columns that exist)\n",
    "daily_metric_cols = [\n",
    "    \"total_steps\", \"total_distance\", \"total_calories\",\n",
    "    \"avg_heart_rate\", \"avg_spo2\",\n",
    "    \"sleep_total_min\", \"cgm_mean\", \"cgm_sd\"\n",
    "] + sleep_cols\n",
    "daily_metric_cols = [c for c in daily_metric_cols if c in daily_summary.columns]\n",
    "\n",
    "for col in daily_metric_cols:\n",
    "    patient_aggs[f\"avg_{col}\"] = (col, \"mean\")\n",
    "\n",
    "qcri_summary = (\n",
    "    daily_summary\n",
    "    .groupby(\"patientID\", as_index=False)\n",
    "    .agg(**patient_aggs)\n",
    ")\n",
    "\n",
    "# Hypo-day percent per patient\n",
    "qcri_summary[\"hypo_day_percent\"] = (qcri_summary[\"hypo_days\"] / qcri_summary[\"total_days\"]) * 100\n",
    "\n",
    "# Round selected columns for presentation\n",
    "for col in qcri_summary.columns:\n",
    "    if col.startswith(\"avg_\") or col in [\"hypo_day_percent\"]:\n",
    "        qcri_summary[col] = qcri_summary[col].round(3)\n",
    "\n",
    "# =========================\n",
    "# VALIDATION TABLE (original + extended)\n",
    "# =========================\n",
    "rows = [\n",
    "    (\"Total Days (avg)\", round(qcri_summary[\"total_days\"].mean(), 3)),\n",
    "    (\"% Hypoglycemic Days (range)\",\n",
    "     f\"{qcri_summary['hypo_day_percent'].min():.3f}% to {qcri_summary['hypo_day_percent'].max():.3f}%\"),\n",
    "]\n",
    "\n",
    "# Keep your original \"Avg. Daily Steps (range)\"\n",
    "if \"avg_total_steps\" in qcri_summary.columns:\n",
    "    rows.append((\"Avg. Daily Steps (range)\",\n",
    "                 f\"{qcri_summary['avg_total_steps'].min():.3f} to {qcri_summary['avg_total_steps'].max():.3f}\"))\n",
    "\n",
    "# Add a few extra ranges for completeness (only if present)\n",
    "extra_ranges = [\n",
    "    (\"Avg. Daily Distance (range)\", \"avg_total_distance\"),\n",
    "    (\"Avg. Daily Calories (range)\", \"avg_total_calories\"),\n",
    "    (\"Avg. Heart Rate (range)\", \"avg_avg_heart_rate\"),\n",
    "    (\"Avg. SpO2 (range)\", \"avg_avg_spo2\"),\n",
    "    (\"Avg. Total Sleep (min) (range)\", \"avg_sleep_total_min\"),\n",
    "]\n",
    "for label, col in extra_ranges:\n",
    "    if col in qcri_summary.columns:\n",
    "        rows.append((label, f\"{qcri_summary[col].min():.3f} to {qcri_summary[col].max():.3f}\"))\n",
    "\n",
    "validation_table = pd.DataFrame(rows, columns=[\"Metric\", \"Your Computation (Per QCRI Style)\"])\n",
    "\n",
    "print(\"\\nQCRI-Style Validation Table:\")\n",
    "print(validation_table.to_string(index=False))\n",
    "\n",
    "# =========================\n",
    "# PLOTS (matplotlib only)\n",
    "# =========================\n",
    "# Boxplot: Daily Steps on Hypo vs Non-Hypo Days\n",
    "if \"total_steps\" in daily_summary.columns:\n",
    "    plt.figure(figsize=(10, 6))\n",
    "    # Prepare data\n",
    "    non_hypo_steps = daily_summary.loc[~daily_summary[\"hypo_day\"], \"total_steps\"].dropna().values\n",
    "    hypo_steps     = daily_summary.loc[daily_summary[\"hypo_day\"], \"total_steps\"].dropna().values\n",
    "    plt.boxplot([non_hypo_steps, hypo_steps], labels=[\"Non-Hypo Day\", \"Hypo Day\"], showmeans=True)\n",
    "    plt.title(\"Boxplot: Daily Step Count on Hypo vs Non-Hypo Days\")\n",
    "    plt.ylabel(\"Total Daily Steps\")\n",
    "    plt.tight_layout()\n",
    "    plt.show()\n",
    "\n",
    "# === Boxplots for multiple daily features vs hypo_day + square collage (no subplots) ===\n",
    "import matplotlib.pyplot as plt\n",
    "from pathlib import Path\n",
    "\n",
    "try:\n",
    "    from PIL import Image\n",
    "    _PIL_OK = True\n",
    "except Exception:\n",
    "    _PIL_OK = False\n",
    "\n",
    "# Use DAILY-LEVEL columns (not per-patient \"avg_*\" ones)\n",
    "_features_to_plot = [\n",
    "    (\"Total Steps\", \"total_steps\"),\n",
    "    (\"Total Distance\", \"total_distance\"),\n",
    "    (\"Total Calories\", \"total_calories\"),\n",
    "    (\"Avg Heart Rate\", \"avg_heart_rate\"),\n",
    "    (\"Avg SpO2\", \"avg_spo2\"),\n",
    "    (\"Total Sleep (min)\", \"sleep_total_min\"),\n",
    "]\n",
    "\n",
    "_out_dir = Path(\"/kaggle/working/hypo_boxplots\")  # change path if you like\n",
    "_out_dir.mkdir(parents=True, exist_ok=True)\n",
    "_saved = []\n",
    "\n",
    "for _label, _col in _features_to_plot:\n",
    "    if _col not in daily_summary.columns:\n",
    "        continue\n",
    "    _non = daily_summary.loc[~daily_summary[\"hypo_day\"], _col].dropna().values\n",
    "    _hyp = daily_summary.loc[daily_summary[\"hypo_day\"],  _col].dropna().values\n",
    "    if len(_non) == 0 and len(_hyp) == 0:\n",
    "        continue\n",
    "\n",
    "    # one figure per feature (no subplots)\n",
    "    plt.figure(figsize=(6, 6))\n",
    "    plt.boxplot([_non, _hyp], labels=[\"Non-Hypo Day\", \"Hypo Day\"], showmeans=True)\n",
    "    plt.title(f\"{_label} on Hypo vs Non-Hypo Days\")\n",
    "    plt.ylabel(_label)\n",
    "    plt.tight_layout()\n",
    "    _out = _out_dir / f\"box_{_col}.png\"\n",
    "    plt.savefig(_out, dpi=150)\n",
    "    plt.close()\n",
    "    _saved.append(_out)\n",
    "\n",
    "# --- Square collage ---\n",
    "_collage_path = _out_dir / \"hypo_vs_nonhypo_boxplots_collage.png\"\n",
    "if _saved and _PIL_OK:\n",
    "    imgs = [Image.open(p).convert(\"RGB\") for p in _saved]\n",
    "    # normalize sizes to smallest tile so grid is neat\n",
    "    _w = min(im.width for im in imgs)\n",
    "    _h = min(im.height for im in imgs)\n",
    "    imgs = [im.resize((_w, _h)) for im in imgs]\n",
    "\n",
    "    import math\n",
    "    n = len(imgs)\n",
    "    rows = int(math.ceil(math.sqrt(n)))\n",
    "    cols = int(math.ceil(n / rows))\n",
    "\n",
    "    canvas = Image.new(\"RGB\", (cols * _w, rows * _h), \"white\")\n",
    "    k = 0\n",
    "    for r in range(rows):\n",
    "        for c in range(cols):\n",
    "            if k >= n:\n",
    "                break\n",
    "            canvas.paste(imgs[k], (c * _w, r * _h))\n",
    "            k += 1\n",
    "    canvas.save(_collage_path)\n",
    "\n",
    "print(\"Saved individual plots:\", [str(p) for p in _saved])\n",
    "print(\"Collage:\", str(_collage_path) if _saved and _PIL_OK else \"Not created\")\n",
    "\n",
    "\n",
    "# Histogram: % Hypoglycemic Days per Patient\n",
    "plt.figure(figsize=(10, 6))\n",
    "vals = qcri_summary[\"hypo_day_percent\"].dropna().values\n",
    "plt.hist(vals, bins=15)\n",
    "plt.axvline(38, linestyle=\"--\", label=\"QCRI Max Threshold (38%)\")\n",
    "plt.title(\"Histogram: % of Hypoglycemic Days per Patient\")\n",
    "plt.xlabel(\"Hypoglycemic Days (%)\")\n",
    "plt.ylabel(\"Patient Count\")\n",
    "plt.legend()\n",
    "plt.tight_layout()\n",
    "plt.show()\n",
    "\n",
    "# Bar plot: each patient's % hypoglycemic days\n",
    "plt.figure(figsize=(12, 6))\n",
    "plt.bar(qcri_summary[\"patientID\"].astype(str), qcri_summary[\"hypo_day_percent\"])\n",
    "plt.title(\"Percentage of Hypoglycemic Days per Patient\")\n",
    "plt.xlabel(\"Patient ID\")\n",
    "plt.ylabel(\"Hypoglycemic Days (%)\")\n",
    "plt.xticks(rotation=45, ha=\"right\")\n",
    "plt.tight_layout()\n",
    "plt.show()\n",
    "\n",
    "# Hour-of-day hypoglycemia rate (all patients, CGM-only)\n",
    "hourly_hypo = (\n",
    "    df_cgm\n",
    "    .groupby(\"hour_of_day\", as_index=False)[\"hypo\"]\n",
    "    .mean()\n",
    "    .sort_values(\"hour_of_day\")\n",
    ")\n",
    "plt.figure(figsize=(12, 6))\n",
    "plt.plot(hourly_hypo[\"hour_of_day\"], hourly_hypo[\"hypo\"], marker=\"o\")\n",
    "plt.title(\"Hourly Hypoglycemia Frequency Across All Patients\")\n",
    "plt.xlabel(\"Hour of Day (0 = Midnight)\")\n",
    "plt.ylabel(\"Hypoglycemia Rate\")\n",
    "plt.xticks(range(0, 24))\n",
    "plt.grid(True)\n",
    "plt.tight_layout()\n",
    "plt.show()\n",
    "\n",
    "# Hourly CGM stats\n",
    "hourly_stats = (\n",
    "    df_cgm\n",
    "    .groupby(\"hour_of_day\")\n",
    "    .agg(cgm_min=(\"cgm\", \"min\"),\n",
    "         cgm_max=(\"cgm\", \"max\"),\n",
    "         cgm_mean=(\"cgm\", \"mean\"),\n",
    "         cgm_std=(\"cgm\", \"std\"))\n",
    "    .reset_index()\n",
    "    .sort_values(\"hour_of_day\")\n",
    ")\n",
    "plt.figure(figsize=(14, 6))\n",
    "plt.plot(hourly_stats[\"hour_of_day\"], hourly_stats[\"cgm_mean\"], marker=\"D\", label=\"Mean CGM\")\n",
    "plt.plot(hourly_stats[\"hour_of_day\"], hourly_stats[\"cgm_std\"], marker=\"^\", label=\"Std Dev CGM\")\n",
    "plt.plot(hourly_stats[\"hour_of_day\"], hourly_stats[\"cgm_min\"], marker=\"s\", label=\"Min CGM\")\n",
    "plt.title(\"Hourly CGM Statistics Across All Patients\")\n",
    "plt.xlabel(\"Hour of Day (0 = Midnight)\")\n",
    "plt.ylabel(\"CGM Value\")\n",
    "plt.xticks(range(0, 24))\n",
    "plt.grid(True)\n",
    "plt.legend()\n",
    "plt.tight_layout()\n",
    "plt.show()\n",
    "\n",
    "# =========================\n",
    "# SAVE OUTPUTS\n",
    "# =========================\n",
    "Path(OUT_DAILY_CSV).parent.mkdir(parents=True, exist_ok=True)\n",
