    "# =========================\n",
    "# BOX PLOTS FOR ALL DAILY FEATURES VS hypo_day\n",
    "# =========================\n",
    "import numpy as np\n",
    "import matplotlib.pyplot as plt\n",
    "from pathlib import Path\n",
    "\n",
    "try:\n",
    "    from PIL import Image\n",
    "    _PIL_OK = True\n",
    "except Exception:\n",
    "    _PIL_OK = False\n",
    "\n",
    "# 1) Fix the earlier typoed boxplot (if you still want a dedicated HR plot)\n",
    "if \"avg_heart_rate\" in daily_summary.columns:\n",
    "    plt.figure(figsize=(10, 6))\n",
    "    non_hypo_hr = daily_summary.loc[~daily_summary[\"hypo_day\"], \"avg_heart_rate\"].dropna().values\n",
    "    hypo_hr     = daily_summary.loc[daily_summary[\"hypo_day\"],  \"avg_heart_rate\"].dropna().values\n",
    "    plt.boxplot([non_hypo_hr, hypo_hr], labels=[\"Non-Hypo Day\", \"Hypo Day\"], showmeans=True)\n",
    "    plt.title(\"Boxplot: Daily Heart Rate on Hypo vs Non-Hypo Days\")\n",
    "    plt.ylabel(\"Average Heart Rate (bpm)\")\n",
    "    plt.tight_layout()\n",
    "    plt.show()\n",
    "\n",
    "# 2) Build the list of candidate daily features from what's available\n",
    "#    (These are columns created in your daily_summary block.)\n",
    "candidate_features = []\n",
    "\n",
    "# totals (heavy-tailed)\n",
    "for c in [\"total_steps\", \"total_distance\", \"total_calories\"]:\n",
    "    if c in daily_summary.columns:\n",
    "        candidate_features.append(c)\n",
    "\n",
    "# physiology\n",
    "for c in [\"avg_heart_rate\", \"avg_spo2\", \"cgm_mean\", \"cgm_sd\", \"hypo_percent\"]:\n",
    "    if c in daily_summary.columns:\n",
    "        candidate_features.append(c)\n",
    "\n",
    "# sleep stages + total sleep\n",
    "sleep_cols_present = [c for c in daily_summary.columns if c.startswith(\"sleep_\") and c.endswith(\"_min\")]\n",
    "candidate_features.extend(sleep_cols_present)\n",
    "if \"sleep_total_min\" in daily_summary.columns:\n",
    "    candidate_features.append(\"sleep_total_min\")\n",
    "\n",
    "# de-duplicate while preserving order\n",
    "seen = set()\n",
    "features_to_plot = []\n",
    "for c in candidate_features:\n",
    "    if c not in seen:\n",
    "        features_to_plot.append(c)\n",
    "        seen.add(c)\n",
    "\n",
    "# 3) Optional: apply log1p to clearly skewed totals (does NOT overwrite your dataframe)\n",
    "log1p_features = {\"total_steps\", \"total_distance\", \"total_calories\"}\n",
    "def maybe_transform(colname, arr):\n",
    "    if colname in log1p_features:\n",
    "        return np.log1p(arr), True\n",
    "    return arr, False\n",
    "\n",
    "# 4) Safe plotting helper\n",
    "def boxplot_feature_by_hypo(df_daily, colname, out_dir):\n",
    "    if colname not in df_daily.columns:\n",
    "        return None\n",
    "\n",
    "    non_vals = df_daily.loc[~df_daily[\"hypo_day\"], colname].astype(float).dropna().values\n",
    "    hyp_vals = df_daily.loc[df_daily[\"hypo_day\"],  colname].astype(float).dropna().values\n",
    "\n",
    "    if len(non_vals) == 0 and len(hyp_vals) == 0:\n",
    "        return None\n",
    "\n",
    "    # optional transform for skewed totals\n",
    "    non_vals_t, did_log = maybe_transform(colname, non_vals)\n",
    "    hyp_vals_t, _       = maybe_transform(colname, hyp_vals)\n",
    "\n",
    "    plt.figure(figsize=(6, 6))\n",
    "    plt.boxplot([non_vals_t, hyp_vals_t], labels=[\"Non-Hypo Day\", \"Hypo Day\"], showmeans=True)\n",
    "    title = f\"{colname} on Hypo vs Non-Hypo Days\"\n",
    "    if did_log:\n",
    "        title += \" (log1p)\"\n",
    "        ylabel = f\"{colname} (log1p)\"\n",
    "    else:\n",
    "        ylabel = colname\n",
    "    plt.title(title)\n",
    "    plt.ylabel(ylabel)\n",
    "    plt.tight_layout()\n",
    "\n",
    "    out_path = out_dir / f\"box_{colname}.png\"\n",
    "    plt.savefig(out_path, dpi=150)\n",
    "    plt.close()\n",
    "    return out_path\n",
    "\n",
    "# 5) Generate & save\n",
    "out_dir = Path(\"/kaggle/working/hypo_boxplots_all\")\n",
    "out_dir.mkdir(parents=True, exist_ok=True)\n",
    "\n",
    "saved_paths = []\n",
    "for col in features_to_plot:\n",
    "    p = boxplot_feature_by_hypo(daily_summary, col, out_dir)\n",
    "    if p is not None:\n",
    "        saved_paths.append(p)\n",
    "\n",
    "print(f\"Saved {len(saved_paths)} boxplots to:\", str(out_dir))\n",
    "\n",
    "# 6) Build a simple collage (optional)\n",
    "collage_path = out_dir / \"collage_boxplots.png\"\n",
    "if saved_paths and _PIL_OK:\n",
    "    # open & normalize sizes\n",
    "    imgs = [Image.open(p).convert(\"RGB\") for p in saved_paths]\n",
    "    w = min(im.width for im in imgs)\n",
    "    h = min(im.height for im in imgs)\n",
    "    imgs = [im.resize((w, h)) for im in imgs]\n",
    "\n",
    "    import math\n",
    "    n = len(imgs)\n",
    "    rows = int(math.ceil(math.sqrt(n)))\n",
    "    cols = int(math.ceil(n / rows))\n",
    "\n",
    "    canvas = Image.new(\"RGB\", (cols * w, rows * h), \"white\")\n",
    "    k = 0\n",
    "    for r in range(rows):\n",
    "        for c in range(cols):\n",
    "            if k >= n: break\n",
    "            canvas.paste(imgs[k], (c * w, r * h))\n",
    "            k += 1\n",
    "    canvas.save(collage_path)\n",
    "    print(\"Collage saved to:\", str(collage_path))\n",
    "else:\n",
    "    print(\"Collage not created (no images or PIL unavailable).\")\n"
   ]
