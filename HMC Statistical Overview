   "source": [
    "import pandas as pd\n",
    "import matplotlib.pyplot as plt\n",
    "\n",
    "# --------------------------------------------------\n",
    "# 1. Load DAILY-LEVEL dataset (per patient per day)\n",
    "# --------------------------------------------------\n",
    "df = pd.read_csv(\"/kaggle/working/daily_summary_qcri.csv\")\n",
    "\n",
    "# Make sure hypo_day is there\n",
    "if \"hypo_day\" not in df.columns:\n",
    "    raise ValueError(\"Column 'hypo_day' not found. Use daily_summary_qcri.csv, not qcri_patient_summary.csv\")\n",
    "\n",
    "# Create a human-readable label\n",
    "df[\"day_type\"] = df[\"hypo_day\"].astype(int).map({0: \"Non-Hypo Day\", 1: \"Hypo Day\"})\n",
    "\n",
    "# --------------------------------------------------\n",
    "# 2. Define feature groups by category\n",
    "# --------------------------------------------------\n",
    "activity_cols = [\n",
    "    \"total_steps\",\n",
    "    \"total_distance\",\n",
    "    \"total_calories\",\n",
    "]\n",
    "\n",
    "cgm_cols = [\n",
    "    \"cgm_mean\",\n",
    "    \"cgm_sd\",\n",
    "    \"hypo_percent\",\n",
    "]\n",
    "\n",
    "cardio_cols = [\n",
    "    \"avg_heart_rate\",\n",
    "    \"avg_spo2\",\n",
    "]\n",
    "\n",
    "sleep_cols = [\n",
    "    \"sleep_awake_min\",\n",
    "    \"sleep_light_min\",\n",
    "    \"sleep_deep_min\",\n",
    "    \"sleep_rem_min\",\n",
    "    \"sleep_total_min\",\n",
    "]\n",
    "\n",
    "def filter_existing(cols):\n",
    "    return [c for c in cols if c in df.columns]\n",
    "\n",
    "activity_cols = filter_existing(activity_cols)\n",
    "cgm_cols      = filter_existing(cgm_cols)\n",
    "cardio_cols   = filter_existing(cardio_cols)\n",
    "sleep_cols    = filter_existing(sleep_cols)\n",
    "\n",
    "print(\"Activity features:\", activity_cols)\n",
    "print(\"CGM features     :\", cgm_cols)\n",
    "print(\"Cardio features  :\", cardio_cols)\n",
    "print(\"Sleep features   :\", sleep_cols)\n",
    "\n",
    "# --------------------------------------------------\n",
    "# 3. Helper: plot category as multi-boxplot\n",
    "# --------------------------------------------------\n",
    "def plot_category_boxplots(cols, title, outfile):\n",
    "    n_feats = len(cols)\n",
    "    if n_feats == 0:\n",
    "        print(f\"No columns found for {title}, skipping.\")\n",
    "        return\n",
    "\n",
    "    plt.figure(figsize=(4 * n_feats, 5))\n",
    "\n",
    "    for i, col in enumerate(cols, 1):\n",
    "        plt.subplot(1, n_feats, i)\n",
    "        df.boxplot(\n",
    "            column=col,\n",
    "            by=\"day_type\",\n",
    "            grid=False,\n",
    "            showmeans=True,\n",
    "            meanprops={\"marker\": \"D\",\n",
    "                       \"markerfacecolor\": \"green\",\n",
    "                       \"markeredgecolor\": \"black\"}\n",
    "        )\n",
    "        plt.xlabel(\"\")\n",
    "        plt.title(col.replace(\"_\", \" \"))\n",
    "\n",
    "    plt.suptitle(title, fontsize=14)\n",
    "    plt.tight_layout(rect=[0, 0, 1, 0.93])\n",
    "    plt.savefig(outfile, dpi=300)\n",
    "    plt.close()\n",
    "    print(f\"Saved {outfile}\")\n",
    "\n",
    "# --------------------------------------------------\n",
    "# 4. Generate the four PNGs\n",
    "# --------------------------------------------------\n",
    "plot_category_boxplots(\n",
    "    activity_cols,\n",
    "    \"Daily activity on hypoglycaemic vs non-hypoglycaemic days\",\n",
    "    \"boxplot_activity.png\",\n",
    ")\n",
    "\n",
    "plot_category_boxplots(\n",
    "    cgm_cols,\n",
    "    \"CGM metrics on hypoglycaemic vs non-hypoglycaemic days\",\n",
    "    \"boxplot_cgm.png\",\n",
    ")\n",
    "\n",
    "plot_category_boxplots(\n",
    "    cardio_cols,\n",
    "    \"Cardiovascular physiology on hypoglycaemic vs non-hypoglycaemic days\",\n",
    "    \"boxplot_cardio.png\",\n",
    ")\n",
    "\n",
    "plot_category_boxplots(\n",
    "    sleep_cols,\n",
    "    \"Sleep structure on hypoglycaemic vs non-hypoglycaemic days\",\n",
    "    \"boxplot_sleep.png\",\n",
    ")\n"
   ]
  }
