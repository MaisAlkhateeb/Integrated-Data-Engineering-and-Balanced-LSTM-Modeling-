   "source": [
    "import pandas as pd\n",
    "import numpy as np\n",
    "\n",
    "# =========================\n",
    "# CONFIG\n",
    "# =========================\n",
    "CSV_PATH = \"/kaggle/input/hmcdataset/intraday.csv\"  # ‚úÖ Change if needed\n",
    "\n",
    "FEATURES = [\n",
    "    \"calories\", \"cgm\", \"deep\", \"distance\", \"heart_rate\",\n",
    "    \"light\", \"nap\", \"rem\", \"spo2\", \"steps\", \"awake\"\n",
    "]\n",
    "ROLLING_SUM_FEATURES = {\"steps\", \"distance\", \"calories\"}\n",
    "\n",
    "# =========================\n",
    "# LOAD & PREP\n",
    "# =========================\n",
    "df = pd.read_csv(CSV_PATH)\n",
    "df[\"start\"] = pd.to_datetime(df.get(\"start\"), errors=\"coerce\")\n",
    "df[\"date\"] = df[\"start\"].dt.date\n",
    "df[\"hour\"] = df[\"start\"].dt.floor(\"h\")\n",
    "\n",
    "cols_present = [\"patientID\", \"date\", \"hour\"] + [c for c in FEATURES if c in df.columns]\n",
    "df_sub = df[cols_present].copy()\n",
    "\n",
    "if \"cgm\" not in df_sub.columns:\n",
    "    raise ValueError(\"`cgm` column is required for hypoglycemia detection but is missing.\")\n",
    "\n",
    "df_cgm = df_sub.dropna(subset=[\"cgm\"]).copy()\n",
    "for c in FEATURES:\n",
    "    if c in df_cgm.columns:\n",
    "        df_cgm[c] = pd.to_numeric(df_cgm[c], errors=\"coerce\")\n",
    "df_cgm[\"hypo\"] = df_cgm[\"cgm\"] <= 70\n",
    "\n",
    "# =========================\n",
    "# DATASET-LEVEL STATS\n",
    "# =========================\n",
    "total_patients = df_cgm[\"patientID\"].nunique()\n",
    "days_per_patient = df_cgm.groupby(\"patientID\")[\"date\"].nunique()\n",
    "avg_days_per_patient = days_per_patient.mean()\n",
    "\n",
    "valid_hours = (\n",
    "    df_cgm.groupby([\"patientID\", \"hour\"])[\"cgm\"]\n",
    "    .count()\n",
    "    .reset_index(name=\"cnt\")\n",
    ")\n",
    "valid_hours = valid_hours[valid_hours[\"cnt\"] >= 4]\n",
    "avg_valid_hours_per_patient = valid_hours.groupby(\"patientID\")[\"hour\"].count().mean()\n",
    "\n",
    "hypo_hours_per_patient = (\n",
    "    df_cgm[df_cgm[\"hypo\"]]\n",
    "    .groupby(\"patientID\")[\"hour\"]\n",
    "    .nunique()\n",
    "    .rename(\"hypo_hours\")\n",
    "    .reset_index()\n",
    ")\n",
    "\n",
    "min_hypo = int(hypo_hours_per_patient[\"hypo_hours\"].min()) if not hypo_hours_per_patient.empty else 0\n",
    "max_hypo = int(hypo_hours_per_patient[\"hypo_hours\"].max()) if not hypo_hours_per_patient.empty else 0\n",
    "avg_hypo = float(hypo_hours_per_patient[\"hypo_hours\"].mean()) if not hypo_hours_per_patient.empty else 0.0\n",
    "\n",
    "# =========================\n",
    "# DISPLAY: Dataset Summary Table\n",
    "# =========================\n",
    "summary_df = pd.DataFrame({\n",
    "    \"Metric\": [\n",
    "        \"Total Patients (with CGM)\",\n",
    "        \"Average Days per Patient (CGM-only)\",\n",
    "        \"Average Valid CGM Hours per Patient (‚â•4/hr)\",\n",
    "        \"Min Hypo Hours per Patient (‚â•1 hypo only)\",\n",
    "        \"Max Hypo Hours per Patient (‚â•1 hypo only)\",\n",
    "        \"Average Hypo Hours per Patient (‚â•1 hypo only)\"\n",
    "    ],\n",
    "    \"Value\": [\n",
    "        round(total_patients, 3),\n",
    "        round(avg_days_per_patient, 3),\n",
    "        round(avg_valid_hours_per_patient, 3),\n",
    "        min_hypo,\n",
    "        max_hypo,\n",
    "        round(avg_hypo, 3)\n",
    "    ]\n",
    "})\n",
    "\n",
    "print(\"\\nüìä **Dataset Summary (CGM-only):**\")\n",
    "print(summary_df.to_markdown(index=False))\n",
    "\n",
    "# =========================\n",
    "# DISPLAY: Per-Feature Summary Table\n",
    "# =========================\n",
    "df_sorted = df_cgm.sort_values([\"patientID\", \"hour\"]).set_index(\"hour\")\n",
    "\n",
    "feature_rows = []\n",
    "\n",
    "for feat in FEATURES:\n",
    "    if feat not in df_sorted.columns:\n",
    "        continue\n",
    "\n",
    "    avg_per_patient = (\n",
    "        df_sorted.groupby(\"patientID\")[feat]\n",
    "        .mean(numeric_only=True)\n",
    "        .dropna()\n",
    "    )\n",
    "    avg_feat_across_patients = float(avg_per_patient.mean()) if not avg_per_patient.empty else np.nan\n",
    "\n",
    "    # Rolling 6h\n",
    "    roll_col = f\"prev6_{feat}\"\n",
    "    if feat in ROLLING_SUM_FEATURES:\n",
    "        df_sorted[roll_col] = (\n",
    "            df_sorted.groupby(\"patientID\")[feat]\n",
    "            .rolling(\"6h\").sum()\n",
    "            .reset_index(level=0, drop=True)\n",
    "        )\n",
    "        roll_agg = \"sum\"\n",
    "    else:\n",
    "        df_sorted[roll_col] = (\n",
    "            df_sorted.groupby(\"patientID\")[feat]\n",
    "            .rolling(\"6h\").mean()\n",
    "            .reset_index(level=0, drop=True)\n",
    "        )\n",
    "        roll_agg = \"mean\"\n",
    "\n",
    "    avg_prev6_before_hypo = float(\n",
    "        df_sorted.loc[df_sorted[\"hypo\"], roll_col].mean()\n",
    "    ) if df_sorted[\"hypo\"].any() else np.nan\n",
    "\n",
    "    feature_rows.append({\n",
    "        \"Feature\": feat,\n",
    "        \"Avg Value per Patient\": round(avg_feat_across_patients, 3),\n",
    "        \"Avg 6h Before Hypo\": round(avg_prev6_before_hypo, 3),\n",
    "        \"6h Aggregation\": roll_agg\n",
    "    })\n",
    "\n",
    "per_feature_df = pd.DataFrame(feature_rows)\n",
    "\n",
    "print(\"\\nüìä **Per-Feature Summary (Across Patients):**\")\n",
    "print(per_feature_df.to_markdown(index=False))\n",
    "\n",
    "# =========================\n",
    "# DISPLAY: Hypo Hours by Patient\n",
    "# =========================\n",
    "print(\"\\nüìä **Hypo Hours by Patient (only those with ‚â•1 hypo):**\")\n",
    "print(hypo_hours_per_patient.to_markdown(index=False))\n"
   ]
  },
