    "# ============================================================\n",
    "# ğŸ” BEST-RECALL MODEL: CONFUSION MATRICES (TEST + BALANCED)\n",
    "#   - Uses sequences_leakfree.npz  (seq + static)\n",
    "#   - Uses results_summary_all.csv (already saved by pipeline)\n",
    "#   - Reuses make_model() and USE_STATIC_INPUT from your script\n",
    "# ============================================================\n",
    "import os\n",
    "import numpy as np\n",
    "import pandas as pd\n",
    "import matplotlib.pyplot as plt\n",
    "from sklearn.metrics import confusion_matrix, ConfusionMatrixDisplay\n",
    "\n",
    "# ---- Paths must match your main script ----\n",
    "OUT_SEQ_NPZ   = \"/kaggle/working/sequences_leakfree.npz\"\n",
    "RESULTS_CSV   = \"/kaggle/working/results_summary_all.csv\"\n",
    "CKPT_FMT      = \"checkpoints/{method}__{model}.h5\"\n",
    "RANDOM_STATE  = 42\n",
    "\n",
    "# -----------------------------\n",
    "# 1) Load sequences + static\n",
    "# -----------------------------\n",
    "if not os.path.exists(OUT_SEQ_NPZ):\n",
    "    raise FileNotFoundError(f\"NPZ with sequences not found: {OUT_SEQ_NPZ}\")\n",
    "\n",
    "npz = np.load(OUT_SEQ_NPZ, allow_pickle=True)\n",
    "\n",
    "Xte      = npz[\"Xte\"]              # (N_test, 36, n_seq_features)\n",
    "yte      = npz[\"yte\"]              # (N_test,)\n",
    "Xte_stat = npz[\"Xte_stat\"]         # may be empty if no static\n",
    "seq_feats   = npz[\"seq_features_used\"]\n",
    "static_feats= npz[\"static_features_used\"]\n",
    "\n",
    "# Handle empty static correctly\n",
    "if Xte_stat.size == 0:\n",
    "    Xte_stat = None\n",
    "\n",
    "seq_len, n_seq_f = Xte.shape[1], Xte.shape[2]\n",
    "n_stat_f = 0 if (Xte_stat is None or not USE_STATIC_INPUT) else Xte_stat.shape[1]\n",
    "\n",
    "print(f\"TEST seq shape: {Xte.shape}, static shape: {None if Xte_stat is None else Xte_stat.shape}\")\n",
    "print(f\"Seq features used: {list(seq_feats)}\")\n",
    "print(f\"Static features used: {list(static_feats)}\")\n",
    "\n",
    "# -----------------------------\n",
    "# 2) Load results and pick BEST recallâ‚ on TEST\n",
    "# -----------------------------\n",
    "if not os.path.exists(RESULTS_CSV):\n",
    "    raise FileNotFoundError(f\"Results CSV not found: {RESULTS_CSV}\")\n",
    "\n",
    "df = pd.read_csv(RESULTS_CSV)\n",
    "\n",
    "# Only TEST rows\n",
    "eval_test = df[df[\"Split\"] == \"test\"].copy()\n",
    "if eval_test.empty:\n",
    "    raise RuntimeError(\"No TEST rows in results CSV. Check 'Split' column contents.\")\n",
    "\n",
    "# Build Brier column if needed\n",
    "if \"Overall/MSE_prob\" in eval_test.columns and \"Brier\" not in eval_test.columns:\n",
    "    eval_test[\"Brier\"] = eval_test[\"Overall/MSE_prob\"]\n",
    "elif \"Brier\" not in eval_test.columns:\n",
    "    eval_test[\"Brier\"] = np.nan\n",
    "\n",
    "# Safe defaults if columns missing\n",
    "for col in [\"Class1/Recall\",\"Overall/F1_weighted\",\"Overall/PR-AUC\"]:\n",
    "    if col not in eval_test.columns:\n",
    "        eval_test[col] = np.nan\n",
    "\n",
    "# Rank: highest Recallâ‚, then F1_weighted, then PR-AUC, then lowest Brier\n",
    "eval_test_sorted = eval_test.sort_values(\n",
    "    by=[\"Class1/Recall\", \"Overall/F1_weighted\", \"Overall/PR-AUC\", \"Brier\"],\n",
    "    ascending=[False, False, False, True]\n",
    ")\n",
    "\n",
    "best_row    = eval_test_sorted.iloc[0]\n",
    "best_method = str(best_row[\"Method\"])\n",
    "best_model  = str(best_row[\"Model\"])\n",
    "best_thr    = float(best_row[\"Threshold\"])\n",
    "\n",
    "print(\"\\n================ BEST TEST RECALL MODEL ================\")\n",
    "print(best_row[[\"Method\",\"Model\",\"Threshold\",\"Class1/Recall\",\n",
    "                \"Overall/F1_weighted\",\"Overall/PR-AUC\",\"Brier\"]])\n",
    "print(\"========================================================\\n\")\n",
    "\n",
    "# -----------------------------\n",
    "# 3) Rebuild model, load weights\n",
    "# -----------------------------\n",
    "ckpt_path = CKPT_FMT.format(method=best_method, model=best_model)\n",
    "if not os.path.exists(ckpt_path):\n",
    "    raise FileNotFoundError(f\"Checkpoint not found for best model: {ckpt_path}\")\n",
    "\n",
    "model_best = make_model(\n",
    "    seq_len=seq_len,\n",
    "    n_seq_f=n_seq_f,\n",
    "    n_stat_f=n_stat_f,\n",
    "    arch=best_model,\n",
    "    lr=1e-3\n",
    ")\n",
    "model_best.load_weights(ckpt_path)\n",
    "print(f\"âœ… Loaded weights from: {ckpt_path}\")\n",
    "\n",
    "# -----------------------------\n",
    "# 4) Confusion matrix on ORIGINAL TEST\n",
    "# -----------------------------\n",
    "if n_stat_f > 0 and USE_STATIC_INPUT and Xte_stat is not None:\n",
    "    p_test = model_best.predict([Xte, Xte_stat], verbose=0).ravel()\n",
    "else:\n",
    "    p_test = model_best.predict(Xte, verbose=0).ravel()\n",
    "\n",
    "yhat_test = (p_test >= best_thr).astype(int)\n",
    "cm_test   = confusion_matrix(yte, yhat_test, labels=[0,1])\n",
    "tn, fp, fn, tp = cm_test.ravel()\n",
    "\n",
    "plt.figure(figsize=(5.5,5))\n",
    "disp = ConfusionMatrixDisplay(confusion_matrix=cm_test, display_labels=[0,1])\n",
    "disp.plot(values_format='d', cmap='Blues', colorbar=False)\n",
    "plt.title(f\"Best Recall Model â€” TEST\\n{best_method}/{best_model} @ thr={best_thr:.2f}\")\n",
    "plt.tight_layout()\n",
    "os.makedirs(\"plots\", exist_ok=True)\n",
    "plt.savefig(\"plots/best_confusion_test.png\", dpi=300)\n",
    "plt.show()\n",
    "\n",
    "print(f\"[BEST RECALL | TEST] {best_method}/{best_model} @ t={best_thr:.2f} \"\n",
    "      f\"â†’ TP={tp}, FP={fp}, TN={tn}, FN={fn}\")\n",
    "print(\"ğŸ–¼ï¸ Saved: plots/best_confusion_test.png\")\n",
    "\n",
    "# -----------------------------\n",
    "# 5) Confusion matrix on BALANCED TEST (with static aligned)\n",
    "# -----------------------------\n",
    "def make_balanced_test_with_static(X, y, X_stat=None, random_state=42):\n",
    "    X = np.asarray(X)\n",
    "    y = np.asarray(y).astype(int).ravel()\n",
    "    idx0 = np.where(y == 0)[0]\n",
    "    idx1 = np.where(y == 1)[0]\n",
    "    if len(idx0) == 0 or len(idx1) == 0:\n",
    "        raise RuntimeError(\"Cannot balance TEST: one of the classes is missing.\")\n",
    "    m = min(len(idx0), len(idx1))\n",
    "    rs = np.random.RandomState(random_state)\n",
    "    keep = np.concatenate([\n",
    "        rs.choice(idx0, m, replace=False),\n",
    "        rs.choice(idx1, m, replace=False)\n",
    "    ])\n",
    "    rs.shuffle(keep)\n",
    "    Xb  = X[keep]\n",
    "    yb  = y[keep]\n",
    "    if X_stat is not None:\n",
    "        Xsb = np.asarray(X_stat)[keep]\n",
    "    else:\n",
    "        Xsb = None\n",
    "    return Xb, yb, Xsb\n",
    "\n",
    "Xte_bal, yte_bal, Xte_stat_bal = make_balanced_test_with_static(\n",
    "    Xte, yte, X_stat=Xte_stat, random_state=RANDOM_STATE\n",
    ")\n",
    "\n",
    "print(f\"Balanced TEST shape: {Xte_bal.shape}, y-dist: {np.bincount(yte_bal)}\")\n",
    "\n",
    "# Predict on balanced TEST\n",
    "if n_stat_f > 0 and USE_STATIC_INPUT and Xte_stat_bal is not None:\n",
    "    p_test_bal = model_best.predict([Xte_bal, Xte_stat_bal], verbose=0).ravel()\n",
    "else:\n",
    "    p_test_bal = model_best.predict(Xte_bal, verbose=0).ravel()\n",
    "\n",
    "yhat_bal = (p_test_bal >= best_thr).astype(int)\n",
    "cm_bal   = confusion_matrix(yte_bal, yhat_bal, labels=[0,1])\n",
    "tn_b, fp_b, fn_b, tp_b = cm_bal.ravel()\n",
    "\n",
    "plt.figure(figsize=(5.5,5))\n",
    "disp_bal = ConfusionMatrixDisplay(confusion_matrix=cm_bal, display_labels=[0,1])\n",
    "disp_bal.plot(values_format='d', cmap='Blues', colorbar=False)\n",
    "plt.title(f\"Best Recall Model â€” BALANCED TEST\\n{best_method}/{best_model} @ thr={best_thr:.2f}\")\n",
    "plt.tight_layout()\n",
    "plt.savefig(\"plots/best_confusion_test_balanced.png\", dpi=300)\n",
    "plt.show()\n",
    "\n",
    "print(f\"[BEST RECALL | BALANCED TEST] {best_method}/{best_model} @ t={best_thr:.2f} \"\n",
    "      f\"â†’ TP={tp_b}, FP={fp_b}, TN={tn_b}, FN={fn_b}\")\n",
    "print(\"ğŸ–¼ï¸ Saved: plots/best_confusion_test_balanced.png\")\n"
   ]
