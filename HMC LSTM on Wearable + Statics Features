      "0         238.178939          221.654394  122.152519  17.134021  1.320985   \n",
      "1         214.850005          192.983329   76.456542   7.109642  2.654828   \n",
      "2         185.525926          171.474074   37.052310  14.231018 -0.777581   \n",
      "3         169.730690          138.102643   -8.187737  -9.873286  2.767731   \n",
      "4         143.462143          122.037857  -44.208146   1.809130  1.884321   \n",
      "\n",
      "   pc1_activity_energy  pc2_physiology  pc3_sleep_rest  hypo_label  \n",
      "0           -61.696963       -2.306252       -4.255645       False  \n",
      "1           -61.696963       -2.306252       -4.255645       False  \n",
      "2           -61.696963       -2.306252       -4.255645       False  \n",
      "3           -61.696963       -2.306252       -4.255645       False  \n",
      "4           -61.696963       -2.306252       -4.255645       False  \n"
     ]
